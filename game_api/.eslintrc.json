{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [2, 4],
        "eol-last": ["error", "never"],
        "max-len": ["error", { "code": 256 }],
        "brace-style": 0,
        "comma-dangle": 0,
        "prefer-const": 0,
        "require-jsdoc": 0,
        "keyword-spacing": 0,
        "no-trailing-spaces": 0,
        "object-curly-spacing": 0
    }
}